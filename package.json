{
    "name": "mmdagent-syntax",
    "displayName": "MMDAgent Syntax",
    "description": "MMDAgent syntax highlighting support",
    "version": "0.0.2",
    "publisher": "C0ela",
    "repository": {
        "type":"git",
        "url": "https://github.com/C0ela/mmdagent-syntax"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mmdagent",
            "aliases": ["MMDAgent", "mmdagent"],
            "extensions": [".fst"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mmdagent",
            "scopeName": "source.mmdagent",
            "path": "./syntaxes/mmdagent.tmLanguage.json"
        }]
    }
}