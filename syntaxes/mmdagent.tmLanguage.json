{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "MMDAgent",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "entity.name.function",
				"match": "(MODEL_ADD|MODEL_CHANGE|MODEL_DELETE|MODEL_EVENT_ADD|MODEL_EVENT_CHANGE|MODEL_EVENT_DELETE)"
			},{
				"name": "entity.name.function",
				"match": "(MOTION_ADD|MOTION_ACCELERATE|MOTION_CHANGE|MOTION_DELETE|MOTION_EVENT_ADD|MOTION_EVENT_ACCELERATE|MOTION_EVENT_CHANGE|MOTION_EVENT_DELETE)"
			},{
				"name": "entitiy.name.function",
				"match": "(MOVE_START|MOVE_STOP|MOVE_EVENT_START|MOVE_EVENT_STOP|TURN_START|TURN_STOP|TURN_EVENT_START|TURN_EVENT_STOP|ROTATE_START|ROTATE_STOP|ROTATE_EVENT_START|ROTATE_EVENT_STOP)"
			},{
				"name": "entitiy.name.function",
				"match": "(SOUND_START|SOUND_STOP|SOUND_EVENT_START|SOUND_EVENT_STOP)"
			},{
				"name": "entitiy.name.function",
				"match": "STAGE"
			},{
				"name": "entitiy.name.function",
				"match": "(LIGHTCOLOR|LIGHTDIRECTION)"
			},{
				"name": "entitiy.name.function",
				"match": "CAMERA"
			},{
				"name": "entitiy.name.function",
				"match": "(RECOG_EVENT_START|RECOG_EVENT_STOP|RECOG_MODIFY)"
			},{
				"name": "entitiy.name.function",
				"match": "(SYNTH_START|SYNTH_STOP|SYNTH_EVENT_START|SYNTH_EVENT_STOP|LIPSYNC_START|LIPSYNC_STOP|LIPSYNC_EVENT_START|LIPSYNC_EVENT_STOP)"
			},{
				"name": "entitiy.name.function",
				"match": "(VALUE_SET|VALUE_UNSET|VALUE_EVAL|VALUE_EVENT_SET|VALUE_EVENT_UNSET|VALUE_EVENT_EVAL|TIMER_START|TIMER_STOP|TIMER_EVENT_START|TIMER_EVENT_STOP)"
			},{
				"name": "entitiy.name.function",
				"match": "(PLUGIN_ENABLE|PLUGIN_DISABLE|PLUGIN_EVENT_ENABLE|PLUGIN_EVENT_DISABLE)"
			},{
				"name": "entitiy.name.function",
				"match": "(DRAGANDDROP|KEY|EXECUTE|KEY_POST)"
			},{
				"name": "variable",
				"match": "<eps>"
			}]
		},
		"strings": {
			"name": "string.quoted.double.mmdagent",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.mmdagent",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.mmdagent"
}